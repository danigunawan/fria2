<h1>New Proposal</h1>
<div class="well">
  <%= form_for @proposal, html: { multipart: true } do |f| %>
    <%= f.label :Title %>
    <%= f.text_field :title %>
    <br><br><%= f.label :Researcher %>
    <%= f.text_field :researcher%>
    <br><br><%= f.label 'File' %>
    <%= f.file_field :avatar, accept: '.pdf' %>
    <%= f.submit "Save", :class => "btn btn-primary" %>
  <% end %>
</div>
